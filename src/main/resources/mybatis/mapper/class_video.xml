<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.mapper.video">

	<select id="selectVideoList" parameterType="int" resultType="video">
		select  * 
		from class_video 
		where class_no = #{class_no}
		order by class_video_no
	</select>
</mapper>
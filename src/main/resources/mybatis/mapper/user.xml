<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.mapper.user">

	<insert  id="insert" parameterType="user">
		insert into member(user_id,user_pw,user_nm,user_nick,user_tel,user_tel,user_pro_img, user_type)
		values(#{user_id},#{user_pw},#{user_nm},#{user_nick},#{user_tel},#{user_tel},#{user_pro_img}, #{user_type})
	</insert>
	

</mapper>